{
  "manifest_version": 3,
  "name": "Webclipper",
  "version": "0.14",
  "browser_specific_settings": {
    "gecko": {
      "id": "webclipper@mangoblogger.com"
    }
  },
  "description": "Keep a history of URLs you browse all the time or sometimes during a research project. Save URLs you browse automatically to your profile on https://mangoblogger.com/accounts/profile. You can access them from anywhere, anytime from a web browser.",
  "homepage_url": "https://mangoblogger.com/apps/webclipper/",
  "icons": {
    "16": "icons/send-tab-url-icon.png",
    "48": "icons/send-tab-url-icon.png",
    "128": "icons/send-tab-url-icon.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "webNavigation",
    "tabs"
  ],
  "background": {
    "scripts": [
      "config.js"
    ]
  },
  "action": {
    "default_popup": "popup/popup3.html",
    "default_icon": {
      "16": "icons/send-tab-url-icon.png",
      "48": "icons/send-tab-url-icon.png",
      "128": "icons/send-tab-url-icon.png"
    }
  },
  "host_permissions": [
    "<all_urls>"
  ]
}